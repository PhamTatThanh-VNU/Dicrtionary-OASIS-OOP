<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.*?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DictionaryController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <HBox prefHeight="47.0" prefWidth="900.0">
         <children>
            <AnchorPane prefHeight="88.0" prefWidth="900.0">
               <children>
                  <Button fx:id="text" layoutX="14.0" layoutY="3.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="52.0" stylesheets="@css/test.css" text="Text" />
                  <Button fx:id="sql" layoutX="89.0" layoutY="2.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="52.0" stylesheets="@css/test.css" text="Sql" />
                  <Button fx:id="gg" layoutX="163.0" layoutY="2.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="52.0" stylesheets="@css/test.css" text="gg" />
               </children></AnchorPane>
         </children>
      </HBox>
    <SplitPane dividerPositions="0.25" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <items>
            <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" />
            <WebView fx:id="textField" prefHeight="200.0" prefWidth="200.0" />
      </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER" prefHeight="33.0" prefWidth="900.0" spacing="5.0" VBox.vgrow="NEVER">
      <children>
            <TextField fx:id="searchBar" prefHeight="26.0" prefWidth="905.0" promptText="Type here" />
            <Button fx:id="searchButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="600.0" text="Search">
               <graphic>
                  <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@add.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
